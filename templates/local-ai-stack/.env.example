# Local AI stack environment (copied to ~/.env by local-ai-starter)
AI_STACK_ENV_FILE=.env
AI_STACK_DATA=/home/your-user/.local/share/nixos-ai-stack

# Model defaults
LLAMA_CPP_DEFAULT_MODEL=unsloth/Qwen3-4B-Instruct-2507-GGUF
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Database credentials
POSTGRES_DB=mcp
POSTGRES_USER=mcp
POSTGRES_PASSWORD=change_me

# Grafana credentials (only needed when --profile full)
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=change_me

# Optional tokens / overrides
HUGGING_FACE_HUB_TOKEN=
STACK_ENV=
